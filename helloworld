#include<stdio.h>
#include<conio.h>
void main()
{
printf("Welcome to Horizon 2k19");
int a,b,c,d,e;
printf("Enter value of a:",a);
scanf("%d",&a);
printf("Enter value of b:",b);
scanf("%d",&b);
c=a+b;
d=a-b;
e=a*b;
printf("The addition is:%d",c);
printf("The subtraction is:%d",d);
printf("The multiplication is:%d",e);
